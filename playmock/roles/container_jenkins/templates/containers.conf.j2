[containers]
netns="host"
userns="host"
ipcns="host"
utsns="host"
cgroupns="host"
cgroups="enabled"
env = [
'http_proxy={{ proxy_env.http_proxy }}',
'https_proxy={{ proxy_env.http_proxy }}',
'no_proxy=localhost,127.0.0.1',
]

[engine]
cgroup_manager = "cgroupfs"
events_logger = "file"
runtime="runc"
env = [
'http_proxy={{ proxy_env.http_proxy }}',
'https_proxy={{ proxy_env.http_proxy }}',
'no_proxy=localhost,127.0.0.1',
]
