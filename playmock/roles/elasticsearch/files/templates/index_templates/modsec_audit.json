{
  "index_patterns": ["fluentd.gooid-web_modsec_audit_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":       { "type": "date" },
        "host":		          { "type": "keyword" },
        "tranzaction_id": 	{ 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },
        "secaudit_parts": 	{ "type": "text" },       
        "message": 		      { "type": "text" }
      }
    }
  }
}
