{
  "index_patterns": ["fluentd.gooid-web_gbs_error_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":   { "type": "date" },
        "host":		      { "type": "keyword" },
        "log-time": 	  { 
          "type": "date",
          "format": "E MMM d HH:mm:ss.SSSSSS yyyy"
        },
        "level": 		    { "type": "keyword" },       
        "pid":          { 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },
        "message":      { "type": "text" }
      }
    }
  }
}
