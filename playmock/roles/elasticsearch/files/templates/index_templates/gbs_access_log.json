{
  "index_patterns": ["fluentd.gooid-web_gbs_access_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":           { "type": "date" },
        "host":		              { "type": "keyword" },
        "remotehost":		        { "type": "ip" },
        "client-identifier": 	  { "type": "text" },
        "user": 	              { "type": "keyword" },
        "log-time": 	          { 
          "type": "date",
          "format": "d/MMM/yyyy:HH:mm:ss Z"
        },
        "size": 		            { "type": "text" },       
        "method":               { "type": "keyword" },
        "path": 		            { "type": "text" },
        "status": 	            { "type": "Keyword" },
        "size2":                { "type": "text" },
        "referer":              { "type": "text" },
        "user-agent":           { "type": "Keyword" }
      }
    }
  }
}
