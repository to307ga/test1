{
  "index_patterns": ["fluentd.common_messages_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":   { "type": "date" },
        "log-time": 	  { 
          "type": "date",
          "format": "MMM d HH:mm:ss||MMM  d HH:mm:ss"
        },
        "hostname": 		{ "type": "keyword" },       
        "service": 		  { "type": "keyword" },
        "message": 		  { "type": "text" }
      }
    }
  }
}
