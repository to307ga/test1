{
  "index_patterns": ["fluentd.gooid-hlp_app_gbs_gbs_webapps_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":     { "type": "date" },
        "server":		      { "type": "keyword" },
        "log-time": 	    { 
          "type": "date",
          "format": "yyyy-MM-dd HH:mm:ss,SSS"
        },
        "msgid": 		      { 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },       
        "level": 		      { "type": "keyword" },
        "message": 		    { "type": "text" }
      }
    }
  }
}
