{
  "index_patterns": ["fluentd.idhub-web_app_idhub_user_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":       { "type": "date" },
        "server":		        { "type": "keyword" },
        "logger": 	        { 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },
        "log-time": 	      { 
          "type": "date",
          "format": "yyyy/MM/dd HH:mm:ss.SSS"
        },
        "thread": 		      { "type": "text" },       
        "file": 		        { "type": "text" },
        "method": 		      { "type": "keyword" },
        "line": 	          { "type": "text" },
        "level": 		        { "type": "keyword" },
        "IP": 		          { "type": "ip" }, 
        "XX": 		          { "type": "text" }, 
        "message_code": 		{ "type": "keyword" }, 
        "status_code": 		  { "type": "keyword" }, 
        "func": 		        { "type": "keyword" }, 
        "site": 		        { "type": "keyword" }, 
        "userid": 		      { "type": "keyword" }, 
        "docomo": 		      { "type": "keyword" }, 
        "ocnid": 		        { "type": "keyword" }, 
        "gooid": 		        { "type": "keyword" }, 
        "msg": 		          { "type": "text" }
      }
    }
  }
}
