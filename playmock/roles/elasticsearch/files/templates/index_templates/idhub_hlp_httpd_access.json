{
  "index_patterns": ["fluentd.idhub-hlp_httpd_access_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":   { "type": "date" },
        "server": 		  { "type": "keyword" },
        "host": 		    { "type": "keyword" },
        "remotelog": 		{ "type": "text" },
        "user": 		    { "type": "text" },
        "log-time": 	  { 
          "type": "date",
          "format": "d/MMM/yyyy:HH:mm:ss Z"
        },
        "size": 		    { "type": "text" },
        "method": 		  { "type": "Keyword" },
        "path": 		    { "type": "text" },
        "status": 		  { "type": "Keyword" },
        "size2": 		    { "type": "text" },
        "referer": 		  { "type": "text" },
        "user-agent": 	{ "type": "Keyword" }
      }
    }
  }
}
