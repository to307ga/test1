{
  "index_patterns": ["fluentd.gooid-web_httpd_access_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":     { "type": "date" },
        "goo_id":		      { 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },
        "host": 		      { "type": "ip" },
        "geoip.location": {
          "type": "geo_point"
        },
        "log-time": 	    {
          "type": "date",
          "format": "dd/MMM/yyyy:HH:mm:ss Z"
        },
        "method": 		    { "type": "keyword" },
        "path": 		      { "type": "text" },
        "referer": 		    { "type": "text" },
        "remotelog": 	    { "type": "text" },
        "server": 		    { "type": "keyword" },
        "size": 		      { "type": "text" },
        "size2": 		      { "type": "text" },
        "status": 		    { "type": "keyword" },
        "user": 		      { "type": "text" },
        "user-agent":	    { "type": "Keyword" }
      }
    }
  }
}
