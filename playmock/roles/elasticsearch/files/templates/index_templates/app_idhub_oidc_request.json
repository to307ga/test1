{
  "index_patterns": ["fluentd.idhub-web_app_idhub_oidc_request_log-*"],
  "priority": 0,
  "composed_of": [
    "index-common-settings"
  ],
  "template": {
    "mappings": {
      "properties": {
        "@timestamp":   { "type": "date" },
        "server":		    { "type": "keyword" },
        "log-time": 	  { 
          "type": "date",
          "format": "date_optional_time"
        },
        "sessionId": 		{ 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },       
        "requestId": 		{ 
          "type":   "text",
          "fields": {
            "raw":  {
              "type": "keyword"
            }
          }
        },
        "level": 		    { "type": "keyword" },
        "message": 	    { "type": "text" }
      }
    }
  }
}
