{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowFirehoseAccess",
      "Effect": "Allow",
      "Principal": {
        "Service": "firehose.amazonaws.com"
      },
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:GetBucketLocation",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:ListBucketMultipartUploads",
        "s3:PutObject"
      ],
      "Resource": [
        "arn:aws:s3:::aws-ses-migration-prod-raw-logs-627642418836",
        "arn:aws:s3:::aws-ses-migration-prod-raw-logs-627642418836/*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:SourceAccount": "627642418836"
        },
        "ArnLike": {
          "aws:SourceArn": "arn:aws:firehose:ap-northeast-1:627642418836:deliverystream/aws-ses-migration-prod-raw-logs-stream"
        }
      }
    }
  ]
}
